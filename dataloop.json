{
    "version": "0.1.28",
    "name": "dataset-insights",
    "displayName": "Dataset Insights",
    "description": "Provides insights on the dataset",
    "attributes": {
        "Provider": "Dataloop",
        "Category": "Application",
        "Application Type": "Data Management",
        "License": "Apache 2.0"
    },
    "codebase": {
        "type": "git",
        "gitUrl": "https://github.com/dataloop-ai-apps/dataset-insights.git",
        "gitTag": "0.1.28"
    },
    "scope": "public",
    "components": {
        "panels": [
            {
                "name": "insights",
                "supportedSlots": [
                    {
                        "type": "datasetBrowser",
                        "configuration": {}
                    }
                ],
                "conditions": {}
            }
        ],
        "toolbars": [
            {
                "name": "insights",
                "displayName": "Insights",
                "icon": "icon-dl-plus",
                "invoke": {
                    "type": "panel",
                    "namespace": "insights"
                },
                "location": "datasetBrowserTab"
            }
        ],
        "modules": [
            {
                "name": "insights",
                "entryPoint": "main.py",
                "className": "Runner",
                "initInputs": [],
                "config": {
                    "isCustomServer": true
                },
                "functions": [
                    {
                        "name": "run",
                        "output": [],
                        "input": []
                    }
                ]
            }
        ],
        "services": [
            {
                "name": "insights-runner",
                "displayName": "insights-runner",
                "moduleName": "insights",
                "panelNames": ["insights"],
                "botUserName": "tab-apps@dataloop.ai",
                "initParams": {},
                "runtime": {
                    "podType": "highmem-l",
                    "numReplicas": 1,
                    "concurrency": 10,
                    "runnerImage": "gcr.io/viewo-g/piper/agent/cpu/dataset-insights:0.0.3",
                    "singleAgent": false,
                    "autoscaler": {
                        "type": "rabbitmq",
                        "minReplicas": 1,
                        "maxReplicas": 1,
                        "queueLength": 1
                    },
                    "preemptible": false,
                    "drainTime": 600,
                    "onReset": "failed",
                    "runExecutionAsProcess": false
                },
                "executionTimeout": 3600,
                "maxAttempts": 3
            }
        ]
    }
}
