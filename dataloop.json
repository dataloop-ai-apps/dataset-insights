{
  "version": "0.1.0",
  "name": "dataset-insights",
  "displayName": "Dataset Insights",
  "description": "Provides insights on the dataset",
  "attributes": {
    "Provider": "Dataloop",
    "Category": "Element",
    "Element Type": "Studio"
  },
  "scope": "public",
  "components": {
    "panels": [
      {
        "name": "insights",
        "supportedSlots": [
          {
            "type": "datasetBrowser",
            "configuration": {}
          }
        ],
        "conditions": {}
      }
    ],
    "toolbars": [
      {
        "displayName": "Insights",
        "icon": "icon-dl-plus",
        "invoke": {
          "type": "panel",
          "namespace": "insights"
        },
        "location": "datasetBrowserTab"
      }
    ],
    "modules": [
      {
        "name": "insights",
        "entryPoint": "app.py",
        "className": "Runner",
        "initInputs": [],
        "config": {
          "isCustomServer": true
        },
        "functions": [
          {
            "name": "run",
            "output": [],
            "input": []
          }
        ]
      }
    ],
    "services": [
      {
        "name": "insights-runner",
        "displayName": "insights-runner",
        "moduleName": "insights",
        "panelNames": [
          "insights"
        ],
        "initParams": {},
        "runtime": {
          "podType": "regular-s",
          "numReplicas": 1,
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/cpu/main:latest",
          "singleAgent": false,
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 1,
            "maxReplicas": 1,
            "queueLength": 1
          },
          "preemptible": false,
          "executionTimeout": 3600,
          "drainTime": 600,
          "onReset": "failed",
          "runExecutionAsProcess": false
        },
        "maxAttempts": 3
      }
    ]
  }
}
